{
  "expo": {
    "name": "Jir√©",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoJire.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "CLOUDINARY_CLOUD_NAME": "dlxshyqra",
      "CLOUDINARY_UPLOAD_PRESET": "mobile_uploads",
      "CLOUDINARY_API_URL": "https://api.cloudinary.com/v1_1",
      "router": {},
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "overlay-swipe",
        "backgroundColor": "#00000000"
      },
      "eas": {
        "projectId": "92a3e563-c5b0-45da-9950-f6195673ea56"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobile"
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#00000000",
      "barStyle": "light-content"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#00000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
